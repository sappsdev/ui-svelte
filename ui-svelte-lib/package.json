{
	"name": "ui-svelte",
	"version": "0.2.11",
	"author": {
		"name": "SappsDev",
		"email": "info@sappsdev.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sappsdev/ui-svelte.git"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"hls.js": "^1.6.15",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.13",
		"shiki": "^3.14.0",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"peerDependencies": {
		"hls.js": "^1.6.15",
		"shiki": "^3.14.0",
		"svelte": "^5.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./css": "./dist/index.css"
	},
	"css": "./dist/index.css",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"homepage": "https://ui-svelte.sappsdev.com",
	"keywords": [
		"svelte"
	],
	"license": "./LICENSE",
	"scripts": {
		"dev": "vite dev",
		"dist": "vite build && bun run prepack",
		"release": "bun run dist && bun publish",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"type": "module",
	"types": "./dist/index.d.ts"
}